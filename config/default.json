{
  "zuix": {
    "build": {
      "input": "./source",
      "output": "./docs",
      "copy": [
        "app",
        "css",
        "images",
        "js",
        ".nojekyll",
        "manifest.json",
        "manifest.webapp",
        "humans.txt",
        "favicon.ico",
        "robots.txt"
      ],
      "ignore": [
        "_inc"
      ],
      "compile": [
        "html",
        "less",
        "css",
        "js",
        "md",
        "svg"
      ],
      "less": true,
      "eslint": true
    },
    "app": {
      "title": "zUIx Web Starter application.",
      "googleSiteId": "UA-123-456",
      "resourcePath": "/app/",
      "libraryPath": "https://genielabs.github.io/zkit/lib"
    }
  }
}
